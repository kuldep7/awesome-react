{
  "name": "@kuldep7/awesome-react",
  "version": "0.0.8",
  "author": "Kuldeep Suthar",
  "license": "ISC",
  "description": "A CLI to scaffold awesome React projects",
  "bin": {
    "car": "./index.mjs"
  },
  "files": [
    "dist",
    "template-main"
  ],
  "type": "module",
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "typecheck": "tsc --noEmit",
    "setup": "npm run build && npx husky init && echo \"npm run lint:staged\" > .husky/pre-commit",
    "lint:fix": "eslint --fix .",
    "lint:staged": "lint-staged",
    "pack": "npm run build && npm pack"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json}": [
      "prettier --write",
      "eslint --fix",
      "git add --force"
    ]
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "dependencies": {
    "minimist": "^1.2.8",
    "prompts": "^2.4.2",
    "kolorist": "^1.8.0",
    "cross-spawn": "^7.0.3"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "@types/minimist": "^1.2.5",
    "@types/prompts": "^2.4.9",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "unbuild": "^2.0.0"
  },
  "publishConfig": {
    "access": "public",
    "@kuldep7:registry": "https://npm.pkg.github.com/"
  }
}
